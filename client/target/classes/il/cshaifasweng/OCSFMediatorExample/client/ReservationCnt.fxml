<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="400.0" prefWidth="600.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.ReservationCntBoundary">
   <children>
      <Button fx:id="CntBtn" layoutX="443.0" layoutY="296.0" mnemonicParsing="false" onAction="#continueAct" prefHeight="49.0" prefWidth="136.0" styleClass="return-btn" text="continue" />
      <Button fx:id="BackBtn" layoutX="20.0" layoutY="296.0" mnemonicParsing="false" onAction="#BackAct" prefHeight="49.0" prefWidth="136.0" styleClass="return-btn" text="back" />
      <Label fx:id="titleLabel" layoutX="126.0" layoutY="14.0" prefHeight="57.0" prefWidth="348.0" styleClass="main-title" text="Please choose available hours">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <ComboBox fx:id="hoursList" layoutX="195.0" layoutY="82.0" onAction="#chooseHours" prefHeight="31.0" prefWidth="210.0" />
      <Label fx:id="noTablesLabel" layoutX="81.0" layoutY="162.0" prefHeight="72.0" prefWidth="459.0" />
   </children>
</AnchorPane>
