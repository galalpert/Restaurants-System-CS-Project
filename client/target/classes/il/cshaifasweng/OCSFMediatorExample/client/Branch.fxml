<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="il.cshaifasweng.OCSFMediatorExample.client.BranchPageController"
            prefHeight="400.0" prefWidth="600.0" stylesheets="@style.css">
    <Label fx:id="branchTitle" layoutX="322.0" layoutY="31.0" prefHeight="63.0" prefWidth="194.0" styleClass="main-title"
           text="Haifa Branch">
        <font>
            <Font size="15.0"/>
        </font>
    </Label>
    <Button fx:id="backToHPBtn" layoutX="603.0" layoutY="21.0" mnemonicParsing="false" onAction="#navToHP"
            prefHeight="49.0" prefWidth="194.0" styleClass="menu-btn" text="Back To Home Page"/>
    <VBox fx:id="sideBar" layoutY="-0.7999999999999989" prefHeight="576.0" prefWidth="203.0" styleClass="sidebar"
          AnchorPane.bottomAnchor="-2.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="611.0"
          AnchorPane.topAnchor="-11.0">
    </VBox>
    <AnchorPane prefHeight="138.0" prefWidth="190.0">
        <ImageView fitHeight="125.0" fitWidth="155.0" layoutX="17.0" pickOnBounds="true" preserveRatio="true"/>
    </AnchorPane>
    <VBox prefHeight="167.0" prefWidth="227.0" spacing="10.0">
        <Button fx:id="reservationBtn" mnemonicParsing="false" onAction="#navToReservationPage" prefHeight="50.0"
                prefWidth="184.0" text="make a reservation">
            <font>
                <Font size="10.0"/>
            </font>
        </Button>
        <Button fx:id="deliveryBtn" mnemonicParsing="false" onAction="#navTodeliveryPage" prefHeight="50.0"
                prefWidth="190.0" text="make a delivery">
            <font>
                <Font size="10.0"/>
            </font>
        </Button>
        <Button fx:id="complaintBtn" mnemonicParsing="false" onAction="#navToComplaintPage" prefHeight="50.0"
                prefWidth="194.0" text="make a complaint">
            <font>
                <Font size="10.0"/>
            </font>
        </Button>
    </VBox>
    <Label fx:id="openingHoursLabel" layoutX="283.0" layoutY="124.0" prefHeight="41.0" prefWidth="136.0"
           styleClass="subtitle" stylesheets="@style.css" text="opening hours:"/>
</AnchorPane>
