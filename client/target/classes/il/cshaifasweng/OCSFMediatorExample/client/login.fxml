<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.LoginBoundary">
   <children>
      <Label fx:id="userLabel" layoutX="131.0" layoutY="94.0" text="User Name :">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label fx:id="passwordLabel" layoutX="131.0" layoutY="156.0" text="Password :">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button fx:id="backToHPBtn" layoutX="14.0" layoutY="22.0" mnemonicParsing="false" onAction="#navToHP" prefHeight="50.0" prefWidth="191.0" styleClass="return-btn" text="Back To Home Page" />
      <TextField fx:id="userNameTextF" layoutX="234.0" layoutY="92.0" onAction="#userNameFiled" />
      <TextField fx:id="passwordTextF" layoutX="234.0" layoutY="154.0" onAction="#passwordFiled" />
      <Button fx:id="loginBtn" layoutX="270.0" layoutY="223.0" mnemonicParsing="false" onAction="#loginFunc" styleClass="menu-btn" text="Log in" />
      <Label fx:id="statusLabel" layoutX="130.0" layoutY="269.0" prefHeight="93.0" prefWidth="338.0" />
   </children>
</AnchorPane>
