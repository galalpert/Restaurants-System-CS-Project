<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.BranchesController">
   <children>
      <Label layoutX="229.0" layoutY="33.0" text="Our Branches Page">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button fx:id="backToHPBtn" layoutX="40.0" layoutY="31.0" mnemonicParsing="false" onAction="#navToHP" text="Back To Home Page" />
      <VBox layoutX="40.0" layoutY="114.0" prefHeight="106.0" prefWidth="107.0">
         <children>
            <Button fx:id="haifaBBtn" mnemonicParsing="false" onAction="#navToHaifaBranch" prefHeight="28.0" prefWidth="125.0" text="Haifa" />
            <Button fx:id="telAvivBtn" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#navToTelAvivBranch" prefHeight="25.0" prefWidth="106.0" text="Tel Aviv" />
            <Button fx:id="jersualemBtn" layoutX="10.0" layoutY="35.0" mnemonicParsing="false" onAction="#navToJersualemBranch" prefHeight="26.0" prefWidth="107.0" text="Jerusalem" />
            <Button fx:id="zikhronBtn" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#navToZikhronBranch" prefHeight="26.0" prefWidth="107.0" text="Zikhron Ya'akov" />
         </children>
      </VBox>
   </children>
</AnchorPane>

